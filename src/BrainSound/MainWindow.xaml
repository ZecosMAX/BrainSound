        <Label x:Name="LabelDevicePID" Content="Label" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelDeviceMID" Content="Label" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelDeviceSPP" Content="Label" HorizontalAlignment="Left" Margin="10,134,0,0" VerticalAlignment="Top"/>
        <Label x:Name="LabelDeviceDV" Content="Label" HorizontalAlignment="Left" Margin="10,165,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="MessagesTextBox" HorizontalAlignment="Left" Height="399" Margin="202,10,0,10" VerticalAlignment="Center" Width="580" FontFamily="Consolas">
        </TextBox>
    </Grid>
</Window>
